{
    "policies": {
        "Cookies": {
            "Behavior": "reject-tracker-and-partition-foreign",
            "BehaviorPrivateBrowsing": "reject-tracker-and-partition-foreign",
            "Allow": [
                "https://amazon.com",
                "https://amazon.nl",
                "https://chatgpt.com",
                "https://controld.com",
                "https://google.com",
                "https://ns.nl",
                "https://web.telegram.org",
                "https://web.whatsapp.com",
                "https://www.imdb.com",
                "https://www.inoreader.com",
                "https://www.zalando.nl",
                "https://github.com/"
            ]
        },
        "SanitizeOnShutdown": {
            "Cache": true,
            "Cookies": true,
            "Sessions": true,
            "History": false,
            "SiteSettings": false
